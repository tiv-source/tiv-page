<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>
  <field name="feedback.cashpoint">
    <field-validator type="requiredstring" short-circuit="true">
      <message key="feedback.validation.cashpoint.required" />
    </field-validator>
    <field-validator type="regex">
      <param name="regex">^[0-9]*$</param>
      <message key="feedback.validation.cashpoint.notanumber" />
    </field-validator>
  </field>

  <field name="feedback.voucher">
    <field-validator type="requiredstring" short-circuit="true">
      <message key="feedback.validation.voucher.required" />
    </field-validator>
    <field-validator type="regex">
      <param name="regex">^[0-9]*$</param>
      <message key="feedback.validation.voucher.notanumber" />
    </field-validator>
  </field>
  
</validators>